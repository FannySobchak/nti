    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.2.1/foundation.min.css">

    {% endblock %}
</head>
<body>
{% block body %}
    <div class="container">
